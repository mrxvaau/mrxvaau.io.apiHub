{
  "name": "api-key-management-system",
  "version": "1.0.0",
  "description": "Full-stack API key management system with Supabase",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "install-all": "npm install && cd client && npm install",
    "setup": "npm run install-all"
  },
  "keywords": [
    "api-key",
    "subscription",
    "supabase",
    "react",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}